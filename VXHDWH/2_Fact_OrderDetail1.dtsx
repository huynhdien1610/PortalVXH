              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	BRPOSItemID,
	BRPOSStoreName,
	BRPOSNo,
	BRPOSs.BRPOSDate,
	BRPOSs.BRPOSStatusCombo,
	BRPOSItemDesc,
	ICProductOF01Combo Size,	
	BRPOSItemQty,
	BRPOSItemUnitPrice,
	BRPOSItemPrice,
	BRPurchaseMethodName,
	(SELECT STUFF( (SELECT ','+ISNULL(ARCampaignDescription,'')
	FROM dbo.BRPOSItemCampaignItems
	JOIN dbo.ARCampaigns ON
	 (ARCampaigns.ARCampaignID = BRPOSItemCampaignItems.FK_ARCampaignID AND ARCampaigns.AAStatus = BRPOSItemCampaignItems.AAStatus)
	WHERE ARCampaigns.AAStatus='Alive' AND BRPOSItemCampaignItems.FK_BRPOSID=BRPOSs.BRPOSID
	GROUP BY ISNULL(ARCampaignDescription,'')
	FOR XML PATH('')),1,1,'')) ARCampaignDescription,
	BRPOSItemDiscAmtTot,
	BRPOSItemPrice-BRPOSItemDiscAmtTot AS SumofTotal,
	BRPOSDutyMealPmtAmt,
	BRPOSSamplePmtAmt
	,AuditUpdate=Getdate()
	,YEAR(BRPOSs.AACreatedDate) * 10000 + MONTH(BRPOSs.AACreatedDate) * 100 + DAY(BRPOSs.AACreatedDate) as PK_DateKey
	,BRPOSItems.BRPOSItemType
	,BRPOSItems.FK_BRPOSItemID
FROM dbo.BRPOSs
JOIN dbo.BRPOSItems 
ON (BRPOSItems.FK_BRPOSID = BRPOSs.BRPOSID AND BRPOSItems.AAStatus = BRPOSs.AAStatus)
LEFT JOIN dbo.ICProducts ON
 (BRPOSItems.FK_ICProductID=ICProducts.ICProductID AND ICProducts.AAStatus = BRPOSItems.AAStatus)
LEFT JOIN dbo.BRPOSStores 
ON (BRPOSs.FK_BRPOSStoreID =BRPOSStores.BRPOSStoreID AND BRPOSStores.AAStatus = BRPOSs.AAStatus)
LEFT JOIN dbo.BRPurchaseMethods
 ON (BRPurchaseMethods.BRPurchaseMethodID = BRPOSs.FK_BRPurchaseMethodID AND BRPurchaseMethods.AAStatus = BRPOSs.AAStatus)
 WHERE BRPOSs.AAStatus='Alive' 
AND  BRPOSDate &gt;= (GETDATE()-7) and BRPOSDutyMealPmtAmt =0 and BRPOSSamplePmtAmt=0</property>
