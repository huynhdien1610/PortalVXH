                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select 
	T2.BRPOSNo
	,T1.BRPOSItemID, T1.FK_BRPOSItemID,T1.BRPOSItemType,T1.BRPOSItemDesc as ExtraName,T1.BRPOSItemQty 
	,T3.BRPOSItemDesc as Product
	,T3.ICProductOF01Combo
	,T6.BRPOSStoreName
        ,T2.AACreatedDate
	,AuditUpdate=Getdate()
	,YEAR(T2.AACreatedDate) * 10000 + MONTH(T2.AACreatedDate) * 100 + DAY(T2.AACreatedDate) as PK_DateKey
	from BRPOSItems as T1
	LEFT JOIN
	(select T5.BRPOSItemID,T5.BRPOSItemDesc,T4.ICProductOF01Combo from BRPOSItems AS T5 
	LEFT JOIN dbo.ICProducts AS T4 ON T5.FK_ICProductID=T4.ICProductID AND T4.AAStatus = T5.AAStatus) 
	AS T3 ON T1.FK_BRPOSItemID =T3.BRPOSItemID
	LEFT JOIN BRPOSs as T2 	ON (T1.FK_BRPOSID = T2.BRPOSID AND T1.AAStatus = T2.AAStatus) 
	LEFT JOIN dbo.BRPOSStores  AS T6
	ON (T2.FK_BRPOSStoreID =T6.BRPOSStoreID AND T6.AAStatus = T2.AAStatus)
