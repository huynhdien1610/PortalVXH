                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select [Year], [MONTH],BRPOSStoreName as [StoreName],Sum(SumofTotalAPIAusNoDisc) as SaleAmount
, sum(Commission) as Commission,sum(VAT)as VAT, Netsale = sum(SumofTotalAPIAusNoDisc-VAT-Commission)
from Fact_OrderSumary
Left join Dim_Date on PK_DateKey= DateKey where BRPOSStatusCombo='Complete' and BRPOSStoreID &lt;&gt; 8 
and [Year] is not null 
Group By  [Year],[MONTH], BRPOSStoreName
order by [Year], [MONTH],BRPOSStoreName</property>
